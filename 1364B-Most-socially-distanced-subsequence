t = int(input())

def sociallyDistanted():
  n = int(input())
  a = list(map(int, input().split()))
  res = [a[0]]
  for i in range(1,n-1):
    before = a[i] - a[i-1]
    after = a[i+1] - a[i]
    if before*after < 0:
      res.append(a[i])
  res.append(a[-1])

  return len(res), (" ".join(map(str, res)))
  
for _ in range(t):
  length, array = sociallyDistanted()
  print(length)
  print(array)
