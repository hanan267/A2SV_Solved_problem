class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        
       # iterate over the first idx string char1 
       # iterate from 1 to last idx char2
       # as going check if same if not break and return till char1 
       # if same continue
       result = ""
       if len(strs) == 1:
          return strs[0]
       for i in range(len(strs[0])):
            for j in range(1,len(strs)):
                if (i>=len(strs[j])) or (strs[0][i] != strs[j][i]) :
                    return result
                
            result += (strs[0][i])

       return result





        
