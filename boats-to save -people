class Solution:
    def numRescueBoats(self, people: List[int], limit: int) -> int:

        people.sort()
        left, right = 0,  len(people)-1
        boats = 0

        while left<=right:
            space = limit-people[right]
            right -= 1
            boats += 1
            if left<=right and space >= people[left]:
                left += 1
        return boats
       
                   





        
